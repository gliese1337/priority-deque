  },
  "homepage": "https://github.com/gliese1337/priority-deque#readme",
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/mocha": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "chai": "^4.3.6",
    "eslint": "^7.15.0",
    "jsmin": "^1.0.1",
    "mocha": "^8.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^3.9.10"
